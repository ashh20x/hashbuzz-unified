    "dev:remote": "vite --mode remotedev",
    "build": "vite build --mode localdev",
    "build:local": "vite build --mode localdev",
    "build:dev": "vite build --mode remotedev",
    "build:cloud": "node --max-old-space-size=4096 --max-semi-space-size=64 ./node_modules/.bin/vite build",
    "preview": "vite preview",
    "serve": "serve -s build -l 3000",
    "start": "vite",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "test": "echo \"Tests not configured yet\" && exit 0",
    "test:watch": "echo \"Tests not configured yet\" && exit 0",
    "test:coverage": "echo \"Tests not configured yet\" && exit 0",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules yarn.lock && yarn install",
    "build:analyze": "vite build --mode analyze",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "validate": "yarn type-check && yarn lint && yarn format:check",
    "prepare": "husky install || true",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "fix-all": "yarn lint:fix && yarn format && echo 'All fixable issues resolved!'",
    "check-all": "yarn type-check && yarn lint && yarn format:check && echo 'All checks passed!'",
    "husky:install": "husky install",
    "husky:uninstall": "husky uninstall"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit --skipLibCheck"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
