{
  "name": "dapp-backend",
  "version": "0.201.23",
  "description": "This service handles all backend-related jobs required for the dApp in the Hashbuzz project.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashbuzz/dApp-backend.git"
  },
  "bugs": {
    "url": "https://github.com/hashbuzz/dApp-backend/issues"
  },
  "homepage": "https://github.com/hashbuzz/dApp-backend#readme",
  "scripts": {
    "build": "npx ts-node build.ts",
    "build:server": "ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "ts-node -r tsconfig-paths/register ./spec",
    "preReadyPrisma": "npx prisma db pull",
    "readyPrisma": "npx prisma generate",
    "db:pull": "npx prisma db pull && npx prisma generate",
    "db:push": "npx prisma db push && npx prisma generate",
    "env-config": "node env.config.js",
    "format": "prettier --write ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@appConfig": "dist/appConfig",
    "@routes": "dist/routes",
    "@middleware": "dist/middleware",
    "@validator": "dist/validator",
    "@smartContract": "dist/smartContract",
    "@controller": "dist/controller",
    "@V201/types": "dist/V201/Types",
    "@V201/modules": "dist/V201/Modules",
    "@V201/events": "dist/V201/AppEvents",
    "@V201/middleware": "dist/V201/Middleware",
    "@V201/Modals":"dist/V201/Modals"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 160
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-explicit-any": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "keywords": [
    "hashbuzz",
    "dapp",
    "backend"
  ],
  "author": "hashbuzz-dev",
  "contributors": [
    {
      "name": "Om Prakash Sah",
      "url": "https://github.com/ops295"
    },
    {
      "name": "Rajat K",
      "url": "https://github.com/rajatK012"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/client-secrets-manager": "^3.616.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@aws-sdk/util-utf8-node": "^3.259.0",
    "@hashgraph/sdk": "^2.30.0",
    "@prisma/adapter-pg": "^6.0.0",
    "@prisma/client": "^6.0.0",
    "@types/joi": "^17.2.2",
    "@types/lusca": "^1.7.5",
    "@types/pg": "^8.11.10",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.7",
    "bignumber.js": "^9.1.2",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "ethers": "^6.13.4",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.0",
    "express-validator": "^6.14.2",
