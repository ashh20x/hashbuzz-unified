    "http-errors": "^2.0.0",
    "http-status-code": "^2.1.0",
    "jet-logger": "^1.2.6",
    "joi": "^18.0.1",
    "jsdoc-to-markdown": "^8.0.0",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "markdown-it": "^13.0.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.44",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.1",
    "node-fetch": "^3.3.1",
